{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "Starter Kit for new web development using react",
  "scripts": {
    "start": "npm-run-all _security-check _lint _open:src",
    "prebuild": "npm-run-all _lint _clean-dist",
    "build": "babel-node tools/build.js",
    "share": "npm-run-all --parallel _open:src _localtunnel",
    "deploy": "echo \"Error: no deploy defined\" && exit 1",
    "_open:src": "babel-node tools/devServer.js",
    "_lint": "eslint webpack.config.js ./src ./tools --color -f table",
    "_security-check": "nsp check",
    "_localtunnel": "lt --port 80 --subdomain xevolvers",
    "_clean-dist": "rimraf ./dist && mkdir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elios264/react-starter.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack2"
  ],
  "author": "elios264",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elios264/react-starter/issues",
    "email": "elios264@hotmail.com"
  },
  "homepage": "https://github.com/elios264/react-starter#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "localtunnel": "^1.8.2",
    "node-sass": "^3.13.0",
    "npm-run-all": "^3.1.2",
    "nsp": "^2.6.2",
    "open": "0.0.5",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.12",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "bootstrap": "^4.0.0-alpha.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "moment": "^2.17.1"
  }
}
